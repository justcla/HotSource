    <CommandPlacement guid="guidTeamExplorerGitCmdSet" id="GitEditor_Context_ThirdGroup" priority="0x0200">
      <Parent guid="guidHotSourcePackageCmdSet" id="FileSourceMenu" />
    </CommandPlacement>
