    <CommandPlacement guid="guidHotSourcePackageCmdSet" id="PendingWindowsGroup" priority="0x0200">
      <Parent guid="guidHotSourcePackageCmdSet" id="SourceControlMenu" />
    </CommandPlacement>
    
    <!-- File-level commands -->
