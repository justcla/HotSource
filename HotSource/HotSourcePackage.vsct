    </CommandPlacement>
    <CommandPlacement guid="guidHotSourcePackageCmdSet" id="UnpushedCommitsCmdId" priority="0x0120">
      <Parent guid="guidHotSourcePackageCmdSet" id="PendingWindowsGroup" />
